{
  "Config": {
    "CsvInputDir": "./",
    "CsvScanRateMs": "1000",
    "CsvRowSeparator": "\n",
    "CsvFieldSeparator": ";"
  },
  "Api": {
    "EndPoint": "",
    "Key": "",
    "EncryptedSecret": ""
  },
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    },
    "Serilog": {
      "Using": [ "Serilog.Sinks.Debug", "Serilog.Sinks.File" ],
      "MinimumLevel": "Information",
      "WriteTo": [
        { "Name": "Debug" },
        {
          "Name": "File",
          "Args": {
            "path": "logs/lsca-.log",
            "rollingInterval": "Day"
          }
        }
      ]
    }
  }
}